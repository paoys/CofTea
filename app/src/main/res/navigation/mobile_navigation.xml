<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_order">

    <fragment
        android:id="@+id/navigation_order"
        android:name="com.example.coftea.Cashier.order.OrderFragment"
        android:label="Order"
        tools:layout="@layout/fragment_order" />

    <fragment
        android:id="@+id/navigation_queue"
        android:name="com.example.coftea.Cashier.queue.QueueFragment"
        android:label="Queue"
        tools:layout="@layout/fragment_queue" />

    <fragment
        android:id="@+id/navigation_stock"
        android:name="com.example.coftea.Cashier.stock.StockFragment"
        android:label="Stock"
        tools:layout="@layout/fragment_stock" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.coftea.Cashier.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
</navigation>